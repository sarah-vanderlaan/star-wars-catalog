{
  "name": "star-wars-characters",
  "version": "1.0.0",
  "description": "A Star Wars Character Catalogue Using a GraphQL wrapper of the Star Wars API",
  "scripts": {
    "start": "npm-run-all --parallel start:server lint:watch test:watch",
    "start:server": "babel-node ./tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src server.js",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Sarah van der Laan",
  "dependencies": {
    "apollo-client": "1.9.3",
    "babel-polyfill": "6.26.0",
    "flexboxgrid": "6.3.1",
    "graphql-tag": "2.5.0",
    "halogen": "0.2.0",
    "react": "15.6.1",
    "react-apollo": "1.4.16",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "css-loader": "0.28.7",
    "enzyme": "2.9.1",
    "eslint": "4.7.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.2",
    "expect": "21.1.0",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "jsdom": "8.5.0",
    "mocha": "3.5.3",
    "node-sass": "4.5.3",
    "npm-run-all": "4.1.1",
    "react-test-renderer": "15.6.1",
    "sass-loader": "6.0.6",
    "sinon": "4.0.1",
    "style-loader": "0.18.2",
    "svg-react-loader": "0.4.5",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sarah-vanderlaan/star-wars-catalog"
  }
}
